    "**/node_modules",
    "**/bundle/**"
  ],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.customHighlight": {
    "DEBUG": {
      "foreground": "#bd2c00",
      "fontWeight": "800"
    },
    "FIXME": {
      "foreground": "#bd2c00",
      "fontWeight": "800"
    },
    "REVIEW": {
      "foreground": "#6e5494",
      "fontWeight": "800"
    },
    "NOTE": {
      "foreground": "#4078c0",
      "fontWeight": "800"
    },
    "TODO": {
      "foreground": "#6cc644",
      "fontWeight": "800"
    },
    "[ ]": {
      "background": "#bd2c0032"
    },
    "[x]": {
      "background": "#6cc64432"
    }
  },
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
