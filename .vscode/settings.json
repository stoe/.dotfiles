{
  "atomKeymap.promptV3Features": true,
  "debug.console.fontFamily": "Menlo, 'DejaVu Sans Mono', 'Lucida Console', monospace",
  "debug.console.fontSize": 16,
  "editor.cursorStyle": "block",
  "editor.detectIndentation": false,
  "editor.fontSize": 16,
  "editor.fontFamily": "Menlo, 'DejaVu Sans Mono', 'Lucida Console', monospace",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 120],
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "indent",
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    ".env": "dotenv",
    "credentials": "properties",
    "*.tf": "terraform",
    "*.config": "ssh_config",
    "config": "ssh_config",
    "known_hosts": "ssh_config",
    "*.ndjson": "ndjson",
    "*.scpt": "applescript",
    ".github/workflows/*.yml": "yaml"
  },
  "files.autoSave": "afterDelay",
  "files.defaultLanguage": "markdown",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true
  },
  "git.confirmSync": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false
  },
  "gitlens.blame.avatars": true,
  "gitlens.blame.highlight.locations": ["gutter", "line", "overview"],
  "gitlens.codeLens.enabled": true,
  "gitlens.currentLine.enabled": true,
  "gitlens.currentLine.scrollable": false,
  "gitlens.hovers.enabled": true,
  "gitlens.settings.mode": "advanced",
  "gitlens.statusBar.reduceFlicker": true,
  "gitlens.views.search.enabled": false,
  "gitlens.views.lineHistory.enabled": true,
  "gitlens.views.repositories.enabled": false,
  "[git-commit]": {
    "editor.rulers": [72, 50],
    "editor.wordWrap": "off"
  },
  "go.gopath": "/Users/stoe/.go",
  "go.useLanguageServer": true,
  "[go]": {
    "editor.snippetSuggestions": "none",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "javascript.referencesCodeLens.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "markdown.extension.toc.levels": "3..4",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "search.exclude": {
    "**/.git": true
  },
  "shellcheck.exclude": [1071],
  "shellcheck.executablePath": "/usr/local/bin/shellcheck",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.shellArgs.osx": ["-l"],
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [".terraform/**/*", "**/.terraform/**/*"]
  },
  "terraform.languageServer": {
    "enabled": false,
    "args": []
  },
  "terraform.telemetry.enabled": false,
  "todo-tree.filtering.ignoreGitSubmodules": true,
  "todo-tree.general.tags": ["DEBUG", "FIXME", "REVIEW", "NOTE", "TODO"],
  "todo-tree.highlights.enabled": false,
  "todo-tree.tree.showScanModeButton": false,
  "todohighlight.keywords": [
    {
      "backgroundColor": "none",
      "color": "#bd2c00",
      "text": "DEBUG"
    },
    {
      "backgroundColor": "none",
      "color": "#bd2c00",
      "text": "FIXME"
    },
    {
      "backgroundColor": "none",
      "color": "#6e5494",
      "text": "REVIEW"
    },
    {
      "backgroundColor": "none",
      "color": "#4078c0",
      "text": "NOTE"
    },
    {
      "backgroundColor": "none",
      "color": "#6cc644",
      "text": "TODO"
    }
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "window.newWindowDimensions": "inherit",
  "window.zoomLevel": 1.15,
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Nord",
  "workbench.editor.labelFormat": "short",
  "workbench.editor.tabCloseButton": "left",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "newUntitledFile",
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  }
}
