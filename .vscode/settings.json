{
  "application.shellEnvironmentResolutionTimeout": 15,
  "atomKeymap.promptV3Features": true,
  "aws.telemetry": false,
  "bookmarks.label.suggestion": "suggestWhenSelected",
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.sideBar.expanded": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "codesnap.boxShadow": "rgba(0, 0, 0, 0) 0px 0px 0px",
  "codesnap.showLineNumbers": false,
  "codesnap.transparentBackground": true,
  "debug.console.fontFamily": "'MesloLGS NF', 'FiraCode Nerd Font Mono', Menlo, 'DejaVu Sans Mono', 'Lucida Console', monospace",
  "debug.console.fontSize": 16,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.organizeImports": false
  },
  "editor.cursorStyle": "block",
  "editor.fontFamily": "'MesloLGS NF', 'FiraCode Nerd Font Mono', Menlo, 'DejaVu Sans Mono', 'Lucida Console', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80,
    120
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": false,
  "editor.inlineSuggest.showToolbar": "always",
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": true,
  "editor.suggest.preview": true,
  "editor.suggest.showMethods": true,
  "editor.tabSize": 2,
  "editor.unicodeHighlight.allowedCharacters": {
    "️": true,
    "ℹ": true
  },
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "deepIndent",
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.associations": {
    ".env*": "dotenv",
    "credentials": "properties",
    "*.tf": "terraform",
    "*config": "ssh_config",
    "known_hosts": "ssh_config",
    ".github/workflows/*.yml": "yaml",
    "**/*ansible*/**/*.yml": "yaml",
    "**/defaults/**/*": "yaml",
    "**/*_vars/**/*.yml": "yaml",
    "**/{handler,inventory,playbooks,roles,tasks,vars}/*/*": "yaml",
    "*.md": "markdown",
    "*.{input,filter,output}.conf": "logstash",
    ".gitconfig*": "properties",
    ".zshrc*": "properties",
    "*.ndjson": "ndjson"
  },
  "files.autoSave": "afterDelay",
  "files.defaultLanguage": "markdown",
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "git.autofetch": false,
  "git.confirmSync": false,
  "git.ignoreMissingGitWarning": true,
  "git.mergeEditor": false,
  "git.openRepositoryInParentFolders": "never",
  "[git-commit]": {
    "editor.rulers": [
      72,
      50
    ],
    "editor.wordWrap": "off"
  },
  "github-actions.org-features": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true,
    "javascript": true
  },
  "githubPullRequests.pullBranch": "never",
  "[github-actions-workflow]": {},
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false
  },
  "gitlens.blame.avatars": true,
  "gitlens.blame.highlight.locations": [
    "gutter",
    "line",
    "overview"
  ],
  "gitlens.codeLens.enabled": true,
  "gitlens.currentLine.enabled": true,
  "gitlens.currentLine.scrollable": false,
  "gitlens.defaultDateFormat": "MMMM D, YYYY HH:mm",
  "gitlens.defaultTimeFormat": "HH:mm ",
  "gitlens.hovers.enabled": true,
  "gitlens.statusBar.reduceFlicker": true,
  "gitlens.views.search.enabled": false,
  "go.gopath": "~/go",
  "go.goroot": "/opt/homebrew/opt/go/libexec",
  "go.toolsManagement.autoUpdate": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "javascript.format.enable": false,
  "javascript.preferences.quoteStyle": "single",
  "javascript.referencesCodeLens.enabled": false,
  "javascript.suggest.includeAutomaticOptionalChainCompletions": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemas": [],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "json.schemaDownload.enable": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.list.indentationSize": "inherit",
  "markdown.extension.toc.githubCompatibility": true,
  "markdown.extension.toc.levels": "3..4",
  "markdown.previewFrontMatter": "show",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "npm.fetchOnlinePackageInfo": false,
  "npm.packageManager": "npm",
  "prettier.arrowParens": "always",
  "prettier.proseWrap": "preserve",
  "prettier.quoteProps": "preserve",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "all",
  "prettier.useTabs": false,
  "python.showStartPage": false,
  "redhat.telemetry.enabled": false,
  "ruby.format": "rubocop",
  "ruby.lint": {
    "rubocop": {
      "useBundler": true
    }
  },
  "ruby.rubocop.configFilePath": "/Users/stoe/code/private/.dotfiles/config/rubocop/config.yml",
  "ruby.rubocop.executePath": "/Users/stoe/.rbenv/shims/",
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "sarif-viewer.connectToGithubCodeScanning": "on",
  "sarif-viewer.explorer.openWhenNoResults": false,
  "search.mode": "reuseEditor",
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "search.useGlobalIgnoreFiles": true,
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredExtensions": [
    "davidanson.vscode-markdownlint",
    "fnando.ruby",
    "kaiwood.endwise",
    "manuelpuyol.erb-linter",
    "mark-tucker.aws-cli-configure",
    "misogi.ruby-rubocop",
    "ms-azuretools.vscode-cosmosdb",
    "ms-sarifvscode.sarif-viewer",
    "ms-vscode.azure-account",
    "ms-vscode.azure-repos",
    "ms-vscode.powershell",
    "rebornix.ruby",
    "vortizhe.simple-ruby-erb",
    "wingrunr21.vscode-ruby",
  ],
  "settingsSync.ignoredSettings": [],
  "shellcheck.executablePath": "/usr/local/bin/shellcheck",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.linux": "zsh (login)",
  "terminal.integrated.defaultProfile.osx": "zsh (login)",
  "terminal.integrated.defaultProfile.windows": "pwsh",
  "terminal.integrated.fontFamily": "'MesloLGS NF', 'FiraCode Nerd Font Mono', Menlo, 'DejaVu Sans Mono', 'Lucida Console', monospace",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.profiles.osx": {
    "zsh (login)": {
      "path": "zsh",
      "args": [
        "-l"
      ],
      "color": "terminal.ansiGreen"
    },
    "bash (login)": {
      "path": "bash",
      "args": [
        "-l"
      ],
      "icon": "terminal-bash",
      "color": "terminal.ansiBlue"
    },
    "pwsh": {
      "path": "/usr/local/bin/pwsh",
      "args": [],
      "icon": "terminal-powershell",
      "color": "terminal.ansiMagenta"
    }
  },
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.tabs.location": "right",
  "terraform.languageServer.path": "/usr/local/bin/terraform",
  "[terraform]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "update.mode": "start",
  "vsicons.dontShowNewVersionMessage": true,
  "window.autoDetectColorScheme": true,
  "window.commandCenter": true,
  "window.newWindowDimensions": "inherit",
  "window.zoomLevel": 0.15,
  "workbench.colorTheme": "GitHub Dark Dimmed",
  "workbench.editor.labelFormat": "short",
  "workbench.editor.tabCloseButton": "left",
  "workbench.editorAssociations": {},
  "workbench.enableExperiments": false,
  "workbench.experimental.settingsProfiles.enabled": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.layoutControl.enabled": true,
  "workbench.layoutControl.type": "both",
  "workbench.preferredDarkColorTheme": "GitHub Dark Dimmed",
  "workbench.preferredLightColorTheme": "GitHub Light Colorblind (Beta)",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "yaml.schemas": {
    "https://json.schemastore.org/github-issue-config.json": "**/.github/ISSUE_TEMPLATE/config.yml"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}
